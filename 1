import java.util.Scanner;

 
class Main
{
  
public static void main (String[]args)

  {
    
Scanner in = new Scanner (System.in);

    
double n, min = 0.0;

    
System.out.print ("Input a number: ");

    
n = in.nextDouble ();

    
min = n;

    
for (int i = 1; i <= 5; i++)
      {
	
System.out.print ("Input a number: ");
	
n = in.nextDouble ();
	
if (n < min)
	  {
	    
min = n;
	  
}
      
}
    
System.out.println ("The smallest number you inputted is: " + min);
  
}

}
